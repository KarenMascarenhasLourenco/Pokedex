<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Pokedex</title>
</head>
<body>
    <header>
        <a href= "/"><h1>Pokedex</h1></a>
        <a href="/cadastro">Cadastre</a>
        <a href="/Pokedex">Pokemons cadastrados</a>
    </header>
    <section class="corpo">
    <% pokedex.forEach(item => { %>
    <section class="pokemonsIniciais">
        <div class="borda">
            <div class="principal">
                <img src=<%=item.imagem%>>
                <p>Número: <%=item.numero%></p>
                <p>Nome: <%=item.nome%></p>
                <p>Tipo: <%=item.tipo%></p>
                <p>Descrição: <%=item.descricao%></p>
                <p>Altura: <%=item.altura%>m</p>
                <p>Peso: <%=item.peso%>kg</p>
                <p>Categoria: <%=item.categoria%></p>
                <p>Habilidade: <%=item.habilidade%></p>
            </div>
        </div>
    </section>
    <% }) %>
</section>
    <footer>Projeto por: Karen Lourenço</footer>
    <script src="/js/script.js"></script>
</body>
</html>